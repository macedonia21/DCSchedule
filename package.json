{
  "name": "meteor-bootstrap4-react-router4-template",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "npm run test-server && npm run test-client",
    "test-server": "TEST_CLIENT=0 SERVER_TEST_REPORTER=\"list\" meteor test --once --driver-package meteortesting:mocha --port 3100",
    "test-client": "NODE_ENV=UNIT_TESTING jest",
    "prettier": "prettier --write \"client/**/*.js\" \"imports/**/*.js\" \"server/**/*.js\" --single-quote true --trailing-comma es5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/enzyme-config.js",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.3.1",
    "jquery": "^3.5.0",
    "loaders.css": "^0.1.2",
    "lodash": "^4.17.11",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-datepicker": "^2.9.6",
    "react-dom": "^16.8.6",
    "react-google-charts": "^3.0.15",
    "react-loaders": "^3.0.1",
    "react-notifications": "^1.4.3",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.5",
    "react-tagsinput": "^3.19.0",
    "recompose": "^0.30.0",
    "simpl-schema": "^1.5.5",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^74.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-meteor": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "regenerator-runtime": "^0.13.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browserlist": [
          "last 2 versions"
        ]
      }
    }
  }
}
